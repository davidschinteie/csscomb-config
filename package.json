{
  "name": "csscomb-config-clicknow",
  "version": "0.0.0-semantically-released",
  "description": "CSScomb shareable config for ClickNow",
  "keywords": [
    "csscomb",
    "csscombconfig",
    "code",
    "quality",
    "style",
    "lint",
    "linter",
    "validate",
    "code style",
    "standard",
    "strict",
    "check",
    "checker",
    "verify",
    "enforce"
  ],
  "homepage": "https://github.com/cknow/csscomb-config-clicknow",
  "bugs": {
    "url": "https://github.com/cknow/csscomb-config-clicknow/issues",
    "email": "ricardo@clicknow.com.br"
  },
  "license": "MIT",
  "author": {
    "name": "Ricardo Gobbo de Souza",
    "email": "ricardogobbosouza@yahoo.com.br",
    "role": "Developer"
  },
  "files": [
    "config.json"
  ],
  "main": "config.json",
  "repository": {
    "type": "git",
    "url": "https://github.com/cknow/csscomb-config-clicknow"
  },
  "scripts": {
    "commit": "git-cz",
    "watch": "nodemon --exec npm t",
    "lint": "eslint test/",
    "pretest": "npm run lint",
    "test": "ava",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm t"
    },
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "commitizen": "^2.8.6",
    "csscomb": "^3.1.7",
    "cz-conventional-changelog": "^1.2.0",
    "eslint": "^3.4.0",
    "eslint-config-clicknow": "^1.6.1",
    "eslint-plugin-ava": "^3.0.0",
    "ghooks": "^1.3.2",
    "nodemon": "^1.10.2",
    "semantic-release": "^4.3.5"
  },
  "peerDependencies": {
    "csscomb": ">=3"
  },
  "engines": {
    "node": ">=4"
  },
  "preferGlobal": false,
  "private": false
}
